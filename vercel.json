{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api(.*)",
      "destination": "/api/index.go"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
